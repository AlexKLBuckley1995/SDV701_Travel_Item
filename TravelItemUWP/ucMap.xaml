<UserControl
    x:Class="TravelItemUWP.ucMap"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:TravelItemUWP"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="300"
    d:DesignWidth="400">

    <Grid>
        <TextBlock x:Name="txbMapType" HorizontalAlignment="Left" Margin="10,10,0,0" TextWrapping="Wrap" Text="Map type" VerticalAlignment="Top"/>
        <TextBlock x:Name="txbMapTypeValue" HorizontalAlignment="Left" Margin="138,10,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>
        <TextBlock x:Name="txbMapDimensions" HorizontalAlignment="Left" Margin="10,50,0,0" TextWrapping="Wrap" Text="Map dimensions" VerticalAlignment="Top"/>
        <TextBlock x:Name="txbMapDimensionValue" HorizontalAlignment="Left" Margin="142,54,0,0" TextWrapping="Wrap" Text="TextBlock" VerticalAlignment="Top"/>

    </Grid>
</UserControl>
